<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="loginVM" type="com.nancysapplication.app.modules.login.data.viewmodel.LoginVM"/>
    </data>
    <FrameLayout android:id="@+id/frameLogin"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylegray_100">
        <ImageView android:id="@+id/imageImgbg"
            android:layout_gravity="top|start"
            android:layout_height="@dimen/_583pxv"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:scaleType="fitXY" imageRes="@{@drawable/img_imgbg}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_imgbg"/>
        <FrameLayout android:id="@+id/frameStackone"
            android:layout_gravity="top|center"
            android:layout_height="@dimen/_570pxv"
            android:layout_marginBottom="@dimen/_48pxv"
            android:layout_marginStart="@dimen/_6pxh"
            android:layout_marginTop="@dimen/_48pxv" android:layout_width="@dimen/_369pxh">
            <ImageView android:id="@+id/imageOne"
                android:layout_gravity="start"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:scaleType="fitXY" imageRes="@{@drawable/img_1}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_1"/>
            <TextView android:id="@+id/txtWelcomeback"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_42pxv"
                android:layout_marginEnd="@dimen/_44pxh"
                android:layout_marginStart="@dimen/_44pxh"
                android:layout_marginTop="@dimen/_42pxv"
                android:layout_width="wrap_content"
                android:letterSpacing="0.02"
                android:text="@{loginVM.loginModel.txtWelcomeback}"
                style="@style/txtPoppinsbold34" tools:text="@string/lbl_welcome_back"/>
        </FrameLayout>
        <FrameLayout android:id="@+id/frameSignInForm"
            android:layout_gravity="bottom|start"
            android:layout_height="@dimen/_439pxv"
            android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearColumnpleasesignin"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_10">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPleaseSignIn"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_49pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_67pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.02"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{loginVM.loginModel.txtPleaseSignIn}"
                    style="@style/txtPoppinsmedium24" tools:text="@string/lbl_please_sign_in"/>
                <EditText android:ellipsize="end"
                    android:hint="@string/msg_irina_volska_gm"
                    android:id="@+id/etEmail" android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_49pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_44pxv"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_8pxh"
                    android:singleLine="true"
                    android:text="@={loginVM.loginModel.etEmailValue}"
                    android:textColorHint="@color/gray_801"
                    style="@style/etRobotoregular14" tools:ignore="Autofill"/>
                <ImageView android:id="@+id/imageFrameTwo"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_marginEnd="@dimen/_49pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_31pxv"
                    android:layout_width="@dimen/_275pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_frame2"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_frame2"/>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearRowfacebook"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_49pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_31pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageFacebook"
                        android:layout_height="@dimen/_40pxh"
                        android:layout_width="@dimen/_40pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_facebook"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_facebook"/>
                    <ImageView android:id="@+id/imageGoogle"
                        android:layout_height="@dimen/_40pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_width="@dimen/_40pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_google"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_google"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtOrcontinueas"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_49pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{loginVM.loginModel.txtOrcontinueas}"
                    style="@style/txtPoppinsmedium10" tools:text="@string/msg_or_continue_as"/>
                <LinearLayout android:id="@+id/linearRowforgotpassword"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_28pxv"
                    android:layout_marginEnd="@dimen/_49pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_37pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtForgotPassword"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.7"
                        android:layout_width="0dp"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{loginVM.loginModel.txtForgotPassword}"
                        style="@style/txtPoppinsmedium16"
                        textUnderline="true" tools:text="@string/msg_forgot_password2"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSignup"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_70pxh"
                        android:layout_weight="0.3"
                        android:layout_width="0dp"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{loginVM.loginModel.txtSignup}"
                        style="@style/txtPoppinsmedium16"
                        textUnderline="true" tools:text="@string/lbl_sign_up3"/>
                </LinearLayout>
            </LinearLayout>
            <ImageView android:id="@+id/imageSignin"
                android:layout_gravity="top|end"
                android:layout_height="@dimen/_78pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_50pxh"
                android:layout_marginStart="@dimen/_50pxh"
                android:layout_width="@dimen/_78pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_signin}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_signin"/>
        </FrameLayout>
        <ImageView android:id="@+id/imageImgbgOne"
            android:layout_gravity="top|start"
            android:layout_height="@dimen/_565pxv"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:scaleType="fitXY" imageRes="@{@drawable/img_imgbg}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_imgbg"/>
        <FrameLayout android:id="@+id/frameStackoneOne"
            android:layout_gravity="top|center"
            android:layout_height="@dimen/_570pxv"
            android:layout_marginBottom="@dimen/_48pxv"
            android:layout_marginStart="@dimen/_6pxh"
            android:layout_marginTop="@dimen/_48pxv" android:layout_width="@dimen/_369pxh">
            <ImageView android:id="@+id/imageOneOne"
                android:layout_gravity="start"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_1_570x369}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_1_570x369"/>
            <TextView android:id="@+id/txtWelcomebackOne"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_42pxv"
                android:layout_marginEnd="@dimen/_44pxh"
                android:layout_marginStart="@dimen/_44pxh"
                android:layout_marginTop="@dimen/_42pxv"
                android:layout_width="wrap_content"
                android:letterSpacing="0.02"
                android:text="@{loginVM.loginModel.txtWelcomebackOne}"
                style="@style/txtPoppinsbold34" tools:text="@string/lbl_welcome_back"/>
        </FrameLayout>
        <FrameLayout android:id="@+id/frameSignInForm1"
            android:layout_gravity="bottom|start"
            android:layout_height="@dimen/_439pxv"
            android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearColumnlogin"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_10">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLogin"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_49pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_67pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.02"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{loginVM.loginModel.txtLogin}"
                    style="@style/txtPoppinsmedium24" tools:text="@string/lbl_login"/>
                <EditText android:ellipsize="end"
                    android:hint="@string/lbl_username"
                    android:id="@+id/etFrameOne"
                    android:inputType="textEmailAddress"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_49pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_44pxv"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_8pxh"
                    android:singleLine="true"
                    android:text="@={loginVM.loginModel.etFrameOneValue}"
                    android:textColorHint="@color/gray_801"
                    style="@style/etRobotoregular14" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPassword"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_58pxh"
                    android:layout_marginStart="@dimen/_58pxh"
                    android:layout_marginTop="@dimen/_31pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{loginVM.loginModel.txtPassword}"
                    style="@style/txtRobotoregular14_2" tools:text="@string/lbl_password"/>
                <LinearLayout android:gravity="center_vertical"
                    android:id="@+id/linearRowfacebookOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_49pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_42pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageFacebookOne"
                        android:layout_height="@dimen/_40pxh"
                        android:layout_width="@dimen/_40pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_facebook"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_facebook"/>
                    <ImageView android:id="@+id/imageGoogleOne"
                        android:layout_height="@dimen/_40pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_width="@dimen/_40pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_google"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_google"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtOrcontinueasOne"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_49pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{loginVM.loginModel.txtOrcontinueasOne}"
                    style="@style/txtPoppinsmedium10" tools:text="@string/msg_or_continue_as"/>
                <LinearLayout
                    android:id="@+id/linearRowforgotpasswordOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_28pxv"
                    android:layout_marginEnd="@dimen/_49pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_37pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtForgotPasswordOne"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.7"
                        android:layout_width="0dp"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{loginVM.loginModel.txtForgotPasswordOne}"
                        style="@style/txtPoppinsmedium16"
                        textUnderline="true" tools:text="@string/msg_forgot_password2"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSignupOne"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_70pxh"
                        android:layout_weight="0.3"
                        android:layout_width="0dp"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{loginVM.loginModel.txtSignupOne}"
                        style="@style/txtPoppinsmedium16"
                        textUnderline="true" tools:text="@string/lbl_sign_up3"/>
                </LinearLayout>
            </LinearLayout>
            <ImageView android:id="@+id/imageSigninOne"
                android:layout_gravity="top|end"
                android:layout_height="@dimen/_78pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_50pxh"
                android:layout_marginStart="@dimen/_50pxh"
                android:layout_width="@dimen/_78pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_signin}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_signin"/>
        </FrameLayout>
    </FrameLayout>
</layout>
