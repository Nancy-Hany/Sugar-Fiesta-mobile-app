<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="onboardingVM" type="com.nancysapplication.app.modules.onboarding.data.viewmodel.OnboardingVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnbackgroundrect"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_60pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:gravity="start"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearOnboarding"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <ImageView android:id="@+id/imageImageremovebg"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_44pxh"
                    android:layout_marginEnd="@dimen/_132pxh"
                    android:layout_marginStart="@dimen/_132pxh"
                    android:layout_width="@dimen/_155pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_imageremovebg"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_imageremovebg"/>
                <FrameLayout android:gravity="start"
                    android:id="@+id/frameStackhomemadeturkis"
                    android:layout_height="@dimen/_809pxv"
                    android:layout_marginTop="@dimen/_60pxv" android:layout_width="match_parent">
                    <FrameLayout
                        android:id="@+id/frameStackbackgroundrect"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_505pxv"
                        android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
                        <ImageView android:id="@+id/imageBackgroundrect"
                            android:layout_gravity="top|start"
                            android:layout_height="@dimen/_463pxv"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            cornerRadius="@{@dimen/_84pxh}"
                            imageRes="@{@drawable/img_backgroundrect_463x430}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_backgroundrect_463x430"/>
                        <FrameLayout
                            android:id="@+id/frameStackrectangleone"
                            android:layout_gravity="bottom|center"
                            android:layout_height="@dimen/_91pxv"
                            android:layout_marginBottom="@dimen/_179pxv"
                            android:layout_marginEnd="@dimen/_48pxh"
                            android:layout_marginStart="@dimen/_48pxh"
                            android:layout_marginTop="@dimen/_179pxv" android:layout_width="@dimen/_332pxh">
                            <ImageView
                                android:id="@+id/imageRectangleOne"
                                android:layout_gravity="start"
                                android:layout_height="@dimen/_91pxh"
                                android:layout_width="match_parent"
                                android:scaleType="fitXY"
                                cornerRadius="@{@dimen/_45pxh}"
                                imageRes="@{@drawable/img_rectangle1}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle1"/>
                            <TextView android:ellipsize="end"
                                android:gravity="top|center"
                                android:id="@+id/txtGetStarted"
                                android:layout_gravity="center"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_27pxv"
                                android:layout_marginEnd="@dimen/_40pxh"
                                android:layout_marginStart="@dimen/_40pxh"
                                android:layout_marginTop="@dimen/_28pxv"
                                android:layout_width="match_parent"
                                android:lineSpacingExtra="@dimen/_1pxh"
                                android:singleLine="true"
                                android:text="@{onboardingVM.onboardingModel.txtGetStarted}"
                                style="@style/txtLatobold36" tools:text="@string/lbl_get_started"/>
                        </FrameLayout>
                    </FrameLayout>
                    <ImageView android:id="@+id/imageHomemadeturkis"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_408pxv"
                        android:layout_marginBottom="@dimen/_32pxv"
                        android:layout_marginTop="@dimen/_32pxv"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_homemadeturkis}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_homemadeturkis"/>
                    <TextView android:gravity="top|center"
                        android:id="@+id/txtStayhealthywh"
                        android:layout_gravity="top|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_51pxh"
                        android:layout_marginStart="@dimen/_51pxh"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_14pxh"
                        android:text="@{onboardingVM.onboardingModel.txtStayhealthywh}"
                        style="@style/txtLatobold36_1" tools:text="@string/msg_stay_healthy_wh"/>
                </FrameLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
