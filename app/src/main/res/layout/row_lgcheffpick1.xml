                        android:layout_width="@dimen/_67pxh"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:paddingBottom="@dimen/_4pxv"
                        android:paddingEnd="@dimen/_8pxh"
                        android:paddingStart="@dimen/_8pxh"
                        android:paddingTop="@dimen/_4pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_20_off"
                        android:textAllCaps="true"
                        style="@style/btnSolidRounded" tools:text="@string/lbl_20_off"/>
                </LinearLayout>
                <TextView android:gravity="center_horizontal"
                    android:id="@+id/txtTimeZoneFour"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:text="@{lgcheffpick1RowModel.txtTimeZoneFour}"
                    style="@style/txtLatoregular12" tools:text="@string/msg_amet_minim_moll"/>
                <LinearLayout android:gravity="end"
                    android:id="@+id/linearExtraInfo"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/_16pxh"
                    android:layout_marginVertical="@dimen/_16pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageStarFour"
                        android:layout_height="@dimen/_9pxh"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_9pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_star"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtReviewScoreFour"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_weight="0.09"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@{lgcheffpick1RowModel.txtReviewScoreFour}"
