                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_weight="0.11"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@{lgcheffpick1RowModel.txtOfReviewsFour}"
                        style="@style/txtLatoregular12_1" tools:text="@string/lbl_34"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtMainCourseTwo"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_weight="0.35"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@{lgcheffpick1RowModel.txtMainCourseTwo}"
                        style="@style/txtLatobold12_1" tools:text="@string/lbl_main_course"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnChefPickTwo"
                        android:layout_height="@dimen/_20pxv"
                        android:layout_marginStart="@dimen/_42pxh"
                        android:layout_width="@dimen/_78pxh"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:paddingBottom="@dimen/_4pxv"
                        android:paddingEnd="@dimen/_8pxh"
                        android:paddingStart="@dimen/_8pxh"
                        android:paddingTop="@dimen/_4pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_chef_pick"
                        android:textAllCaps="true"
                        style="@style/btnSolidRounded_1" tools:text="@string/lbl_chef_pick"/>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </FrameLayout>
</layout>
