<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signupTwoVM" type="com.nancysapplication.app.modules.signuptwo.data.viewmodel.SignupTwoVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumntermandcondition"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_75pxv"
        android:layout_marginEnd="@dimen/_52pxh"
        android:layout_marginStart="@dimen/_45pxh"
        android:layout_marginTop="@dimen/_60pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:gravity="start"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearSignUpTwo"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <ImageView android:id="@+id/imageImageremovebg"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_44pxh"
                    android:layout_marginEnd="@dimen/_48pxh"
                    android:layout_marginStart="@dimen/_48pxh"
                    android:layout_width="@dimen/_155pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_imageremovebg"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_imageremovebg"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_radius_24"
                    android:ellipsize="end"
                    android:hint="@string/lbl_username"
                    android:id="@+id/etButton" android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_58pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_18pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_27pxh"
                    android:paddingTop="@dimen/_19pxv"
                    android:singleLine="true"
                    android:text="@={signupTwoVM.signupTwoModel.etButtonValue}"
                    android:textColorHint="@color/gray_900_7e"
                    style="@style/etSolidRounded" tools:ignore="Autofill"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_radius_24"
                    android:ellipsize="end"
                    android:hint="@string/lbl_email_address"
                    android:id="@+id/etButtonOne"
                    android:inputType="textEmailAddress"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_36pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_19pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_18pxv"
                    android:singleLine="true"
                    android:text="@={signupTwoVM.signupTwoModel.etButtonOneValue}"
                    android:textColorHint="@color/gray_900_7e"
                    style="@style/etSolidRounded" tools:ignore="Autofill"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_radius_24"
                    android:ellipsize="end"
                    android:hint="@string/lbl_password"
                    android:id="@+id/etButtonTwo"
                    android:inputType="textPassword"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_36pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_19pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_27pxh"
                    android:paddingTop="@dimen/_18pxv"
                    android:singleLine="true"
                    android:text="@={signupTwoVM.signupTwoModel.etButtonTwoValue}"
                    android:textColorHint="@color/gray_900_7e"
                    style="@style/etSolidRounded" tools:ignore="Autofill"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_radius_24"
                    android:ellipsize="end"
                    android:hint="@string/msg_confirm_passwor"
                    android:id="@+id/etButtonThree"
                    android:inputType="textPassword"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_36pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_19pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_27pxh"
                    android:paddingTop="@dimen/_18pxv"
                    android:singleLine="true"
                    android:text="@={signupTwoVM.signupTwoModel.etButtonThreeValue}"
                    android:textColorHint="@color/gray_900_7e"
                    style="@style/etSolidRounded" tools:ignore="Autofill"/>
                <TextView android:id="@+id/txtTermAndCondition"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_width="match_parent"
                    android:text="@{signupTwoVM.signupTwoModel.txtTermAndCondition}"
                    style="@style/txtPoppinssemibold20" tools:text="@string/msg_by_registering"/>

                <com.asksira.loopingviewpager.indicator.CustomShapePagerIndicator
                    android:id="@+id/indicatorArrowdown"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_48pxh"
                    android:layout_marginStart="@dimen/_48pxh"
                    android:layout_marginTop="@dimen/_31pxv"
                    android:layout_width="wrap_content"
                    app:drawable_normal="@drawable/indicator_unselected_bg_red_300_7f_radius_8"
                    app:drawable_selected="@drawable/indicator_selected_bg_red_301_radius_8"
                    app:indicator_size="@dimen/_12pxh" app:indicator_spacing="@dimen/_10pxh"/>

                <com.asksira.loopingviewpager.indicator.CustomShapePagerIndicator
                    android:id="@+id/indicatorTelevision"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_131pxh"
                    android:layout_marginStart="@dimen/_131pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="wrap_content"
                    app:drawable_normal="@drawable/indicator_unselected_bg_white_a700_7f_radius_8"
                    app:drawable_selected="@drawable/indicator_selected_bg_white_a700_radius_8"
                    app:indicator_size="@dimen/_12pxh" app:indicator_spacing="@dimen/_10pxh"/>
                <FrameLayout android:id="@+id/frameStackbackgroundrect"
                    android:layout_height="@dimen/_92pxv"
                    android:layout_marginTop="@dimen/_30pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageBackgroundrect"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_92pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_46pxh}"
                        imageRes="@{@drawable/img_backgroundrect}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_backgroundrect"/>
                    <FrameLayout
                        android:id="@+id/frameStackrectangleone"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_91pxv"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginEnd="@dimen/_1pxh" android:layout_width="@dimen/_332pxh">
                        <ImageView android:id="@+id/imageRectangleOne"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_91pxh"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            cornerRadius="@{@dimen/_45pxh}"
                            imageRes="@{@drawable/img_rectangle1}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle1"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSignUp"
                            android:layout_gravity="bottom|center"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_23pxv"
                            android:layout_marginEnd="@dimen/_110pxh"
                            android:layout_marginStart="@dimen/_110pxh"
                            android:layout_marginTop="@dimen/_23pxv"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{signupTwoVM.signupTwoModel.txtSignUp}"
                            style="@style/txtLatobold36" tools:text="@string/lbl_signup2"/>
                    </FrameLayout>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtYoucanalsosi"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_48pxh"
                    android:layout_marginStart="@dimen/_48pxh"
                    android:layout_marginTop="@dimen/_29pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{signupTwoVM.signupTwoModel.txtYoucanalsosi}"
                    style="@style/txtLatobold20" tools:text="@string/msg_you_can_also_si"/>
                <ImageView android:id="@+id/imageGroupEightyThree"
                    android:layout_height="@dimen/_27pxh"
                    android:layout_marginEnd="@dimen/_85pxh"
                    android:layout_marginStart="@dimen/_85pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="@dimen/_148pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group79"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group79"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
